{
  "src": {
    "app": {
      "home": {
        "home.component.css": ".fondo {\r\n  background-image: linear-gradient(147deg, #e7e7e7 10%, #66b6df 80%);\r\n  min-height: 820px;\r\n}\r\n\r\n.card {\r\n  opacity: 0;\r\n  transform: translateY(100px);\r\n  animation: slideIn 0.8s forwards ease-in-out;\r\n}\r\n\r\n/* Animación de entrada */\r\n@keyframes slideIn {\r\n  0% {\r\n    opacity: 0;\r\n    transform: translateY(50px);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n.card:hover {\r\n  transition: transform 0.3s ease-in-out;\r\n  transform: scale(1.05);\r\n}\r\n",
        "home.component.html": "<section class=\"w-full py-12 md:py-24 lg:py-32 fondo\">\r\n  <div\r\n    class=\"container mx-auto grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-6 px-4 md:px-6 justify-center\"\r\n  >\r\n    <!-- Tarjeta Mayor Menor -->\r\n    <div\r\n      class=\"relative card hover:scale-105 transition-all duration-300 transform grid h-[40rem] w-full max-w-[28rem] items-end justify-center overflow-hidden text-center rounded-lg\"\r\n    >\r\n      <div\r\n        class=\"absolute inset-0 m-0 h-full w-full rounded-lg bg-cover bg-center\"\r\n        style=\"background-image: url('assets/img/H&L.png')\"\r\n      >\r\n        <div\r\n          class=\"absolute inset-0 h-full w-full rounded-lg bg-gradient-to-t from-black/80 via-black/50\"\r\n        ></div>\r\n      </div>\r\n      <div class=\"relative py-14 px-6 md:px-12\">\r\n        <h2 class=\"mb-6 text-3xl font-medium leading-[1.5] text-white\">\r\n          Mayor Menor\r\n        </h2>\r\n        <p class=\"mb-4 text-lg text-white\">Juego de adivinanzas de números</p>\r\n        <button\r\n          class=\"mt-4 px-6 py-2 bg-blue-600 text-white rounded-lg\"\r\n          [routerLink]=\"['/mayor-menor']\"\r\n        >\r\n          Jugar\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Tarjeta Preguntados -->\r\n    <div\r\n      class=\"relative card hover:scale-105 transition-all duration-300 transform grid h-[40rem] w-full max-w-[28rem] items-end justify-center overflow-hidden text-center rounded-lg\"\r\n    >\r\n      <div\r\n        class=\"absolute inset-0 m-0 h-full w-full rounded-lg bg-cover bg-center\"\r\n        style=\"background-image: url('assets/img/Preguntados.png')\"\r\n      >\r\n        <div\r\n          class=\"absolute inset-0 h-full w-full rounded-lg bg-gradient-to-t from-black/80 via-black/50\"\r\n        ></div>\r\n      </div>\r\n      <div class=\"relative py-14 px-6 md:px-12\">\r\n        <h2 class=\"mb-6 text-3xl font-medium leading-[1.5] text-white\">\r\n          Preguntados\r\n        </h2>\r\n        <p class=\"mb-4 text-lg text-white\">Trivia de conocimiento general</p>\r\n        <button\r\n          class=\"mt-4 px-6 py-2 bg-blue-600 text-white rounded-lg\"\r\n          [routerLink]=\"['/preguntados']\"\r\n        >\r\n          Jugar\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Tarjeta Tetris -->\r\n    <div\r\n      class=\"relative card hover:scale-105 transition-all duration-300 transform grid h-[40rem] w-full max-w-[28rem] items-end justify-center overflow-hidden text-center rounded-lg\"\r\n    >\r\n      <div\r\n        class=\"absolute inset-0 m-0 h-full w-full rounded-lg bg-cover bg-center\"\r\n        style=\"background-image: url('assets/img/Tetris.jpg')\"\r\n      >\r\n        <div\r\n          class=\"absolute inset-0 h-full w-full rounded-lg bg-gradient-to-t from-black/80 via-black/50\"\r\n        ></div>\r\n      </div>\r\n      <div class=\"relative py-14 px-6 md:px-12\">\r\n        <h2 class=\"mb-6 text-3xl font-medium leading-[1.5] text-white\">\r\n          Tetris\r\n        </h2>\r\n        <p class=\"mb-4 text-lg text-white\">Clásico juego de bloques</p>\r\n        <button\r\n          class=\"mt-4 px-6 py-2 bg-blue-600 text-white rounded-lg\"\r\n          [routerLink]=\"['/tetris']\"\r\n        >\r\n          Jugar\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Tarjeta Ahorcado -->\r\n    <div\r\n      class=\"relative card hover:scale-105 transition-all duration-300 transform grid h-[40rem] w-full max-w-[28rem] items-end justify-center overflow-hidden text-center rounded-lg\"\r\n    >\r\n      <div\r\n        class=\"absolute inset-0 m-0 h-full w-full rounded-lg bg-cover bg-center\"\r\n        style=\"background-image: url('assets/img/ahorcado.png')\"\r\n      >\r\n        <div\r\n          class=\"absolute inset-0 h-full w-full rounded-lg bg-gradient-to-t from-black/80 via-black/50\"\r\n        ></div>\r\n      </div>\r\n      <div class=\"relative py-14 px-6 md:px-12\">\r\n        <h2 class=\"mb-6 text-3xl font-medium leading-[1.5] text-white\">\r\n          Ahorcado\r\n        </h2>\r\n        <p class=\"mb-4 text-lg text-white\">Juego de palabras</p>\r\n        <button\r\n          class=\"mt-4 px-6 py-2 bg-blue-600 text-white rounded-lg\"\r\n          [routerLink]=\"['/ahorcado']\"\r\n        >\r\n          Jugar\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</section>\r\n",
        "home.component.spec.ts": "import { ComponentFixture, TestBed } from '@angular/core/testing';\r\n\r\nimport { HomeComponent } from './home.component';\r\n\r\ndescribe('HomeComponent', () => {\r\n  let component: HomeComponent;\r\n  let fixture: ComponentFixture<HomeComponent>;\r\n\r\n  beforeEach(async () => {\r\n    await TestBed.configureTestingModule({\r\n      imports: [HomeComponent]\r\n    })\r\n    .compileComponents();\r\n\r\n    fixture = TestBed.createComponent(HomeComponent);\r\n    component = fixture.componentInstance;\r\n    fixture.detectChanges();\r\n  });\r\n\r\n  it('should create', () => {\r\n    expect(component).toBeTruthy();\r\n  });\r\n});\r\n",
        "home.component.ts": "import { Component } from '@angular/core';\r\nimport { RouterModule } from '@angular/router';\r\n\r\n@Component({\r\n  selector: 'app-home',\r\n  standalone: true,\r\n  imports: [RouterModule],\r\n  templateUrl: './home.component.html',\r\n  styleUrls: ['./home.component.css']\r\n})\r\nexport class HomeComponent {}\r\n"
